<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/PrivacyAndTerms"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupActivity">

    <EditText
        android:id="@+id/ConfirmPassword"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Confirm Password"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="521dp" />

    <EditText
        android:id="@+id/Password"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Password"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="476dp" />

    <EditText
        android:id="@+id/PhoneNumber"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Phone Number "
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="431dp" />

    <EditText
        android:id="@+id/LastName"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Last Name"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="341dp" />

    <EditText
        android:id="@+id/FirstName"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="First Name"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="289dp" />

    <EditText
        android:id="@+id/Email"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Email"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="386dp" />

    <Button
        android:id="@+id/Create"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:text="Create Account"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="596dp" />

    <SearchView
        android:id="@+id/SearchView"
        android:layout_width="59dp"
        android:layout_height="65dp"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="31dp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="371dp"
        android:layout_height="74dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="By signing up you agree to the Third Space Privacy Policy and Terms of Condition"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="705dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="CREATE AN ACCOUNT"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="169dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/Third"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="31dp" />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="79dp"
        android:layout_height="61dp"
        app:srcCompat="@drawable/house"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="823dp" />

</androidx.constraintlayout.widget.ConstraintLayout>